{
  "name": "rephub",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "express": "^4.18.2",
    "googleapis": "^118.0.0",
    "markdown-it": "^13.0.1",
    "markdown-it-chords": "^1.0.0",
    "node-fetch": "^2.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "toml": "^3.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "quality-check": "npm run lint && npm run test:ci && npm run build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --coverage --watchAll=false --passWithNoTests=false",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix --max-warnings=0",
    "type-check": "echo 'No TypeScript in this project'",
    "eject": "react-scripts eject",
    "server": "node server.js",
    "dev": "npm run build && npm run server",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "pre-push": "npm run test:ci && npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.4.24",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  }
}
